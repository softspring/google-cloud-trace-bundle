{
    "name": "softspring/google-cloud-trace-bundle",
    "description": "This bundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=8.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.14",
        "phpstan/phpstan": "^1.9"
    },
    "autoload": {
        "psr-4": {
            "Softspring\\GoogleCloudTraceBundle\\": "src"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-5.1": "5.1.x-dev"
        }
    }
}
